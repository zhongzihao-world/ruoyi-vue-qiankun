    "core-js": "3.19.1",
    "element-ui": "2.15.8",
    "interactjs": "^1.10.14",
    "js-cookie": "3.0.1",
    "vue": "2.6.12",
    "vue-router": "3.4.9",
    "vuedraggable": "2.24.3",
    "vuex": "3.6.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.4.6",
    "@vue/cli-plugin-eslint": "4.4.6",
    "@vue/cli-service": "4.4.6",
    "babel-eslint": "10.1.0",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "babel-plugin-import": "^1.13.5",
    "chalk": "4.1.0",
